        PARAMETER[\(dqFalse northing\(dq,10000000,
            LENGTHUNIT[\(dqmetre\(dq,1],
            ID[\(dqEPSG\(dq,8807]]],
    CS[Cartesian,2],
        AXIS[\(dq(E)\(dq,east,
            ORDER[1],
            LENGTHUNIT[\(dqmetre\(dq,1]],
